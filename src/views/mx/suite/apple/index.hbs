                    <button onclick="play()" class="play-button-initial2" id="buttonVideoplay"><figure class="play-icon-initial"></figure></button>
                    <button onclick="pause()" class="pause-button-initial2" id="buttonVideopause"><figure class="pause-icon-initial"></figure></button>
